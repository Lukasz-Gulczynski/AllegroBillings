{
  "AllegroApiSettings": {
    "BaseUrl": "https://api.allegro.pl",
    "GetBillingsEndpoint": "/billing/billing-entries",
    "Token": "bearer-token"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=allegro_billing.db"
  },
  "Quartz": {
    "quartz.scheduler.instanceName": "QuartzWithCore",
    "quartz.scheduler.instanceId": "instance_one",
    "quartz.threadPool.type": "Quartz.Simpl.SimpleThreadPool, Quartz",
    "quartz.threadPool.threadCount": "5",
    "quartz.threadPool.threadPriority": "Normal",
    "quartz.jobStore.misfireThreshold": "60000",
    "quartz.jobStore.type": "Quartz.Impl.AdoJobStore.JobStoreTX, Quartz",
    "quartz.jobStore.driverDelegateType": "Quartz.Impl.AdoJobStore.SQLiteDelegate, Quartz",
    "quartz.jobStore.dataSource": "default",
    "quartz.dataSource.default.provider": "SQLite-40",
    "quartz.dataSource.default.connectionString": "Data Source=quartz_jobs.db"
  }
}
